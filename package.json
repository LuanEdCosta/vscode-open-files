{
  "name": "vscode-open-files",
  "displayName": "Open Files",
  "version": "0.1.0",
  "license": "MIT",
  "publisher": "Luan Eduardo da Costa",
  "description": "You can open related files, all files in a folder, open file in the browser and configure files to open together",
  "main": "./dist/extension.js",
  "repository": "https://github.com/LuanEdCosta/vscode-open-files",
  "homepage": "https://github.com/LuanEdCosta/vscode-open-files",
  "author": {
    "name": "Luan Eduardo da Costa",
    "url": "https://github.com/LuanEdCosta"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-open-files.openAllFolderFiles",
    "onCommand:vscode-open-files.openRelatedFiles",
    "onCommand:vscode-open-files.openFileInBrowser"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-open-files.openAllFolderFiles",
        "title": "Open all folder files"
      },
      {
        "command": "vscode-open-files.openRelatedFiles",
        "title": "Open related files"
      },
      {
        "command": "vscode-open-files.openFileInBrowser",
        "title": "Open file in browser"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "vscode-open-files.openFileInBrowser",
          "group": "OpenFiles@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "eslint:lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "yarn eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "clean:dist": "rimraf dist",
    "clean:modules": "rimraf node_modules",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.1",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "glob": "^7.1.7",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5",
    "vscode-test": "^1.6.1",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  }
}
